{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
  },
  "globals": {
    "angular": true,
    "ngRoute": true,
  },
  "rules": {
    "quotes": [2, "single"],
    "comma-dangle": [1, "always-multiline"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "no-console": [1, {
      allow: ["warn", "error", "log"],
    }],
  },
}